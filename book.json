{
    "title": "伊桑老师的文档",
    "author": "Ethan.Shen",
    "description": "伊桑老师的文档",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {}
    },
    "plugins": [
        "-sharing",
        "-highlight",
        "prism",
        "splitter",
        "expandable-chapters-small",
        "-anchors",
        "github",
        "github-buttons",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon-absolute",
        "copy-code-button"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/a-ethan"
        },
        "github-buttons": {
            "buttons": [{
                "user": "a-ethan",
                "repo": "docs-huncloud-cn",
                "type": "star",
                "size": "small",
                "count": true
                }
            ]
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                "google","facebook","weibo","twitter","linkedin"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon-absolute":{
            "favicon": "/images/favicon.ico",
            "bookmark": "/images/favicon.ico",
            "appleTouchIcon152": "/apple-touch-icon-152.png",
            "appleTouchIconPrecomposed152": "/apple-touch-icon-precomposed-152.png",
            "appleTouchIconMore": {
                "120x120": "/apple-touch-icon-120.png",
                "180x180": "/apple-touch-icon-180.png"
            },
            "appleTouchIconPrecomposedMore": {
                "120x120": "/apple-touch-icon-precomposed-120.png",
                "180x180": "/apple-touch-icon-precomposed-180.png"
            }
        },
        "prism": {
            "css": [
              "../../css/prism-coldark-cold.css"
            ]
          }
    }
}